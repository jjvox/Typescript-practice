{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": ["src/"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)_"
  },
  "sourceType": "module",
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",               
    "destination": "./out/",          
    "recurse": true              
  }
}